# set path to password store
root= ~/.password-store

alias column=/home/zeltak/programs/column



# optional rofi parameters:
rofiopts="-i -no-levenshtein-sort"


# fields to be used
URL_field='URL'
USERNAME_field='UserName'
AUTOTYPE_field='autotype'

## Programs to be used
# Editor
EDITOR='emacsclient'

# Browser
BROWSER='chromium-browser'

## Misc settings

help_color='#0C73C2'
auto_enter='false'

# Custom Keybindings
autotype="Alt+1"
type_user="Alt+2"
type_pass="Alt+3"
open_url="Alt+4"
copy_name="Alt+c"
copy_pass="Alt+Shift+c"
edit="Alt+1"
move="Alt+2"
delete="Alt+3"
show="Control+Return"
copy_entry="Alt+2"
type_entry="Alt+1"
